function carialici(){
	global $conn,$dbnetsis; ///select count(*) as ALICILAR from tblcasabit where CARI_TIP = 'A'
	$q = q("select count(*) as ALICILAR from {$dbnetsis}.dbo.TBLCASABIT Where CARI_TIP = 'A'");
	if(qn($q) != 0)
	{
		$infocarialicilar = qa($q);
		$alicilar = floor(($infocarialicilar['ALICILAR']));
	}
	return $alicilar;
}
